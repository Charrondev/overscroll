{
    "name": "overscrolljs",
    "version": "1.0.4",
    "description": "Detect Mac touchpad overscroll on browser",
    "main": "dist/overscroll.js",
    "scripts": {
        "build:typescript": "tsc",
        "build:minify": "uglifyjs -o dist/overscroll.min.js --compress --mangle -- dist/overscroll.js",
        "build": "npm run build:typescript && npm run build:minify",
        "prepublishOnly": "npm run build"
    },
    "types": "dist/overscroll.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bluzi/overscroll.git"
    },
    "keywords": [
        "overscroll",
        "mac",
        "mabook",
        "touchpad",
        "scroll"
    ],
    "author": "Eliran Pe'er",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/bluzi/overscroll/issues"
    },
    "homepage": "https://github.com/bluzi/overscroll#readme",
    "devDependencies": {
        "typescript": "^2.5.3",
        "uglify-js": "^3.1.3"
    }
}
